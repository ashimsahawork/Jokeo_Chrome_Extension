{
  "manifest_version": 3,
  "name": "Jokeo",
  "version": "1.0",
  "description": "Get a chuckle with Jokeo! Click for a random joke!",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "16": "images/logo_16.png",
    "32": "images/logo_32.png",
    "48": "images/logo_48.png",
    "128": "images/logo_128.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/logo_16.png",
      "32": "images/logo_32.png",
      "48": "images/logo_48.png",
      "128": "images/logo_128.png"
    }
  },
  "background": {
    "service_worker": "script/background.js"
  }
}